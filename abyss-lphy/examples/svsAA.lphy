λ ~ LogNormal(meanlog=1.5, sdlog=0.2);
ψ ~ Yule(lambda=λ, n=20);
rates ~ Dirichlet(conc=rep(element=1.0,times=190));
k ~ LogNormal(meanlog=0.5, sdlog=0.2);
a ~ LogNormal(meanlog=0.5, sdlog=0.5);
indicators ~ ConnectedSVS(states=20, sens=k, scale=a, symmetric=true);
freq ~ Dirichlet(conc=rep(4.0,20));
Q = nonReversible(rates=rates, freq=freq, indicators=indicators, symmetric=true);
D ~ PhyloCTMC(L=1000, Q=Q, tree=ψ, dataType=aminoAcids());
